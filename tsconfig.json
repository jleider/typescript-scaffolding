{
  "compilerOptions": {
    "baseUrl": ".",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["dom", "es2015"],
    "module": "es2015",
    "moduleResolution": "node",
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "outDir": "dist/",
    "paths": {},
    "preserveConstEnums": true,
    "pretty": true,
    "strict": true,
    "sourceMap": true,
    "target": "es5",
    "types": []
  },
  "files": [
    "src/index.ts"
  ]
}


